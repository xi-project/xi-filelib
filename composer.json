{
    "name": "xi/filelib",
    "type": "library",
    "description": "File library for PHP 5.3",
    "keywords": ["file management", "file", "image", "video"],
    "homepage": "http://github.com/xi-project/xi-filelib",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mikko ForsstrÃ¶m",
            "email": "pekkisx@gmail.com",
            "homepage": "http://diktaattoriporssi.com",
            "role": "Developer"
        },
        {
            "name": "Mikko Hirvonen",
            "email": "mikko.petteri.hirvonen@gmail.com",
            "role": "Developer"
        }
    ],

    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        },
        {
            "type": "package",
            "package": {
                "name": "zencoder/zencoder-php",
                "version": "2.0.2",
                "source": {
                    "url": "git://github.com/zencoder/zencoder-php.git",
                    "type": "git",
                    "reference": "v2.0.2"
                }
            }
        },
        {
            "type": "vcs",
            "url": "git://github.com/peterhil/ZendService_Amazon.git"
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "symfony/event-dispatcher": ">=2.0.14, <=2.1.2"
    },

    "require-dev": {
        "doctrine/orm": "2.2.2",
        "doctrine/dbal": "2.2.2",
        "doctrine/common": "2.2.2",
        "symfony/http-foundation": "2.1.2",
        "symfony/http-kernel": "2.1.2",
        "symfony/process": "2.1.2",
        "symfony/security": "2.1.2",
        "videlalvaro/php-amqplib": ">=dev-master",
        "zencoder/zencoder-php": "2.0.2",
        "zendframework/zendservice-amazon": "dev-fix-s3-client",
        "zendframework/zend-i18n": "2.0.*",
        "zendframework/zend-filter": "2.0.*",
        "zendframework/zend-servicemanager": "2.0.*"
    },

    "autoload": {
        "psr-0": {
            "Xi\\Filelib": "library/",
            "Xi\\Tests": "tests/"
        }
    },
    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "0.8.x-dev"
        }
    }


}
